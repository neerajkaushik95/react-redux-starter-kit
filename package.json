    "html-webpack-plugin": "3.2.0",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "mockdate": "2.0.2",
    "ngrok": "^3.1.1",
    "node-sass": "7.0.0",
    "opn-cli": "3.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "^1.17.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-app-polyfill": "^1.0.0",
    "react-test-renderer": "16.4.2",
    "redux-mock-store": "1.5.3",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "1.1.1",
    "webpack": "4.17.1",
